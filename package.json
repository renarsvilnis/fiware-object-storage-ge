    "eslint-loader": "1.3.0",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-react": "5.1.1",
    "eslint-plugin-standard": "1.3.2",
    "husky": "0.11.4",
    "mime": "1.3.4",
    "node-uuid": "^1.4.7",
    "nodemon": "1.9.2",
    "nyc": "6.4.4"
  },
