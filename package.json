  ],
  "homepage": "https://github.com/renarsvilnis/fiware-object-storage-ge",
  "repository": {
    "type": "git",
    "url": "https://github.com/renarsvilnis/fiware-object-storage-ge.git"
  },
  "license": "MIT",
  "dependencies": {
    "co": "4.6.0",
    "debug": "^2.2.0",
    "superagent": "^1.6.1"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "babel-core": "6.9.1",
    "babel-eslint": "6.0.4",
